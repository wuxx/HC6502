000000r 1               ; void led_init()
000000r 1               ; Initialize the LED VIA pin.
000000r 1               ; Initially the LED is turned off.
000000r 1  AD 23 7F     _led_init:  lda LED_DDR
000003r 1  09 80                    ora #LED
000005r 1  8D 23 7F                 sta LED_DDR
000008r 1  AD 21 7F                 lda LED_OUT
00000Br 1  29 7F                    and #<~LED
00000Dr 1  8D 21 7F                 sta LED_OUT
000010r 1  60                       rts
000011r 1               
000011r 1               ; void led_set(char state)
000011r 1               ; Set the LED on (state != 0) or off (state == 0)
000011r 1               ; @in A (state) turns the LED off, any other value turns it on
000011r 1  C9 00        _led_set:   cmp #0
000013r 1  D0 09                    bne @on
000015r 1  AD 21 7F     @off:       lda LED_OUT
000018r 1  29 7F                    and #<~LED
00001Ar 1  8D 21 7F                 sta LED_OUT
00001Dr 1  60                       rts
00001Er 1  AD 21 7F     @on:        lda LED_OUT
000021r 1  09 80                    ora #LED
000023r 1  8D 21 7F                 sta LED_OUT
000026r 1  60                       rts
